<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.search.SearchDiseaseResultActivity"
    >
    <include layout="@layout/view_toolbar"/>
    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
        <com.think.awhealth.widget.TitleWithContentView
            android:id="@+id/id_titleWithContent_sysmtext"
            android:layout_marginTop="?attr/actionBarSize"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <com.think.awhealth.widget.TitleWithContentView
            android:id="@+id/id_titleWithContent_caretext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <com.think.awhealth.widget.TitleWithContentView
            android:id="@+id/id_titleWithContent_causetext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <com.think.awhealth.widget.TitleWithContentView
            android:id="@+id/id_titleWithContent_checks"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <com.think.awhealth.widget.TitleWithContentView
            android:id="@+id/id_titleWithContent_checktext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <com.think.awhealth.widget.TitleWithContentView
            android:id="@+id/id_titleWithContent_food"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <com.think.awhealth.widget.TitleWithContentView
            android:id="@+id/id_titleWithContent_foodtext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>

</FrameLayout>
